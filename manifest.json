{
    "manifest_version": 2,
    "name": "Google link fixer",
    "version": "1",

    "default_locale": "en",
    "description": "Prevents Google from changing the URL of the search result when you click on it",
    "icons": { "128": "icon_128.png" },

    "author": "littlelightlittlefire",
    "content_scripts": [
        {
            "matches": ["*://*.google.*"],
            "js": ["content.js"]
        }
    ],

    "homepage": "https://github.com/littlelightlittlefire/chrome-link-fixer",
    "minimum_chrome_version": 44
}
